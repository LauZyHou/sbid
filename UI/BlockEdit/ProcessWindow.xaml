<Window x:Class="sbid.UI.ProcessWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sbid.UI"
        mc:Ignorable="d"
        Title="编辑进程模板-" Height="300" Width="500" Topmost="True" ResizeMode="NoResize">
    <TabControl Name="TabControl1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <TabItem Header="Attribute">
            <Grid Background="#FFE5E5E5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50*"/>
                    <ColumnDefinition Width="50*"/>
                </Grid.ColumnDefinitions>
                <DockPanel Grid.Column="0">
                    <Border BorderBrush="Black" BorderThickness="0,0,1,0"/>
                </DockPanel>
                <DockPanel Grid.Column="1">
                    <Border BorderBrush="Black"  BorderThickness="1,0,0,0"/>
                </DockPanel>
            </Grid>
        </TabItem>
        <TabItem Header="Method">
            <Grid Background="#FFE5E5E5">
                <TextBox Margin="0" TextWrapping="Wrap" Text="" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
            </Grid>
        </TabItem>
        <TabItem Header="CommMethod">
            <Grid Background="#FFE5E5E5">
                <TextBox Margin="0" TextWrapping="Wrap" Text="" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
            </Grid>
        </TabItem>
    </TabControl>
</Window>
