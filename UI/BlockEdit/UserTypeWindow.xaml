<Window x:Class="sbid.UI.UserTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sbid.UI"
        mc:Ignorable="d"
        Title="编辑" Height="450" Width="800">
       <TabControl Name="TabControl1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <TabItem Header="Attribute">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="50*"/>
                        <ColumnDefinition Width="50*"/>
                    </Grid.ColumnDefinitions>
                    <DockPanel Grid.Column="0">
                        <Border BorderBrush="Black" BorderThickness="0,0,1,0"/>
                    </DockPanel>
                    <DockPanel Grid.Column="1">
                        <Border BorderBrush="Black"  BorderThickness="1,0,0,0"/>
                    </DockPanel>
                </Grid>
            </TabItem>
            
        </TabControl>
</Window>
